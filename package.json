{
  "name": "run",
  "version": "1.0.0",
  "description": "Run or walk for your transport?",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm run build & nodemon server.js",
    "build": "watchify src/app.jsx -t [ babelify --presets [ react ] ] -o public/bundle.js -v",
    "build-test": "browserify test/test.js -o test/test-bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "test": "node_modules/mocha/bin/mocha --compilers js:babel/register test/test.js --reporter nyan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minaorangina/run.git"
  },
  "keywords": [
    "tfl",
    "transport",
    "journey",
    "commute",
    "productivity"
  ],
  "author": "minaorangina",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "hapi": "^11.1.4",
    "inert": "^3.2.0",
    "jquery": "^2.1.4",
    "moment": "^2.10.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "request": "^2.67.0",
    "soap": "^0.11.1"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "jsdom": "^7.2.1",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "react-addons-test-utils": "^0.14.3",
    "watchify": "^3.6.1"
  }
}
