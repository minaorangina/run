{
  "name": "run",
  "version": "1.0.0",
  "description": "Run or walk for your transport?",
  "main": "./server/start.js",
  "scripts": {
    "start": "node ./server/start.js",
    "dev": "npm run build & node ./src/utils/dev-server.js",
    "build": "webpack --config webpack.dev.config.js",
    "test": "tape --require babel-register --require ignore-styles --require ./test/utils/test_helpers.js 'test/tape/**/*.es6'",
    "test:mocha": "node_modules/mocha/bin/mocha --compilers js:babel-core/register --require ./test/utils/test_helpers.js 'test/unit/**/*.@(js|jsx)'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minaorangina/run.git"
  },
  "keywords": [
    "tfl",
    "transport",
    "journey",
    "commute",
    "productivity"
  ],
  "author": "minaorangina",
  "license": "MIT",
  "dependencies": {
    "hapi": "^11.1.4",
    "inert": "^3.2.0",
    "jquery": "^2.1.4",
    "material-ui": "^0.15.0-alpha.1",
    "moment": "^2.10.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "request": "^2.67.0",
    "soap": "^0.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.6.5",
    "chai": "^3.4.1",
    "css-loader": "^0.23.1",
    "faucet": "0.0.1",
    "ignore-styles": "^1.2.0",
    "jsdom": "^3.1.2",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "on-build-webpack": "^0.1.0",
    "react-addons-test-utils": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "tape": "^4.5.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
